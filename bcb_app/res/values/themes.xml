<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- the theme applied to the application or activity -->
    <style name="CustomActionBarThemee" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="android:actionBarStyle" tools:targetApi="11">@style/MyActionBare</item>

        <!-- Support library compatibility -->
        <item name="actionBarStyle">@style/MyActionBare</item>
    </style>

    <!-- ActionBar styles -->
    <style name="MyActionBare" parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@drawable/actionbar_background</item>

        <!-- Support library compatibility -->
        <item name="background">@drawable/actionbar_background</item>
        
        
        
    </style>

</resources>